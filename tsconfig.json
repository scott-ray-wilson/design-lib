{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "jsx": "react",
    "sourceMap": true,
//    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

    // bundle types
//    "declaration": true,
//    "declarationDir": "dist/types",
//    "emitDeclarationOnly": true
  },
  "exclude": [
    "./types/**/*" // This is what fixed it!
  ]
}
